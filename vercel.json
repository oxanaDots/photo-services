{
    "version": 2,
    "builds": [
      {
        "src": "booking-backend/server.ts",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "booking-backend/server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  
  