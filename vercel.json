{
  "version": 2,


  "builds": [
    {
      "src": "booking-backend/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["booking-backend/**/*.ts"],
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
  
    {
      "source": "/signup", 
      "destination": "booking-backend/server.ts"
    },
    {
      "source": "/signin", 
      "destination": "booking-backend/server.ts"    },
    {
      "source": "/submit", 
      "destination": "booking-backend/server.ts"    },
    {
      "source": "/(.*)",
     "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}
