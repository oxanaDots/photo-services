{
    "version": 2,
    "builds": [
      {
        "src": "booking-backend/server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "path/to/other/files/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "booking-backend/server.ts"
      }
    ]
  }
  